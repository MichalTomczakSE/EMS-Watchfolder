{
  "watchFolder": "/path/",
  "transcodingFolder": "/path/",
  "destinationFolder": "/path/",
  "sequenceFolder" : "/path/",
  "input": "-i",
  "scale": "-s",
  "resolution":"1920x1080",
  "filter": "-filter_complex",
  "filterOptions": "[1:v][0:v]overlay=0:0[out]",
  "mapper": "-map",
  "mapOutput": "[out]",
  "setVideoCodec": "-c:v",
  "videoCodec":"libx264",
  "setVideoBitrate": "-b:v",
  "videoBitrate": "5M"
}


