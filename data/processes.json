{
  "watchFolder": "/path/",
  "transcodingFolder": "/path/",
  "destinationFolder": "/path/",
  "sequenceFolder" : "/path/",
  "input": "-i",
  "framerate": "-framerate",
  "framerateValue": "30",
  "scale": "-s",
  "resolution":"1920x1080",
  "filter": "-filter_complex",
  "filterOptions": "[1:v][0:v]overlay=0:0[out]",
  "mapper": "-map",
  "mapOutput": "[out]",
  "setVideoCodec": "-c:v",
  "videoCodec":"libx264",
  "setVideoBitrate": "-b:v",
  "videoBitrate": "10M",
  "videoFilter": "-vf",
  "sampleAspectRatio": "scale=1920:1080,setsar=1"
}
